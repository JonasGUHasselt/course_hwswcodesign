#include "print.h"

unsigned int get_hamming_weight(unsigned int x);

void main(void) {
	unsigned int value, hw;

	value = 208;

	hw = get_hamming_weight(value);

	print_str("HW(0x");
	print_hex(value, 2);
	print_str(") = 0x");
	print_dec(hw);
}

unsigned int get_hamming_weight(unsigned int x) {
    unsigned char hw = 0, i;

	for(i=0;i<32;i++) {
		if(x & 0x00000001) {
			hw++;
		}
		x = x >> 1;
	}

	return hw;
}