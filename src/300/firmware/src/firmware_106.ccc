#include "print.h"

unsigned int get_factorial(unsigned int x);

void main(void) {
	volatile unsigned int value1, factorial;

	value1 = 8;

	factorial = get_factorial(value1);

	print_dec(factorial);
}

unsigned int get_factorial(unsigned int x) {
    unsigned int temp, i;

    if(x==1) return 1;
    if(x==2) return 2;

    temp = 1;

    for(i=2;i<=x;i++) {
        temp = temp * i;
    }

    return temp;
}